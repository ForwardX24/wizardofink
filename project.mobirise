set type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/printm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#000000\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/printm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n            <div class=\"col-12 col-lg-5\" mbr-if=\"showLogo || showTitle || showText\">\n                <div class=\"title-wrapper\">\n                    <div class=\"logo-wrapper\" mbr-if=\"showLogo\">\n                        <img src=\"@PROJECT_PATH@/assets/images/whatsapp-image-2024-06-24-at-14.02.03-430x429.jpg\" alt=\"Mobirise\">\n                    </div>\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .logo-wrapper\"><b>The Wizard&nbsp;</b><br><b>Of Ink</b></h2>\n                    <div class=\"text-wrapper\">\n                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .text-wrapper\"></p>\n                    </div>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-5\">\n                <div class=\"content-wrapper\" mbr-if=\"showNav || showContacts || showSocial\">\n                    <div class=\"nav-wrapper\" mbr-if=\"showNav\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list, .item-wrap, .nav-wrapper, .social-wrapper\" data-multiline mbr-article>\n                            <li class=\"item-wrap\"><b>Home</b></li>\n                            <li class=\"item-wrap\"><b>Portfolio</b></li>\n                            <li class=\"item-wrap\"><b>Contact.&nbsp; &nbsp; &nbsp; Over ons</b></li>\n                        </ul>\n                    </div>\n                    <div class=\"contacts-wrapper\" mbr-if=\"showContacts\">\n                        <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list, .item-wrap, .social-wrapper\" data-multiline mbr-article>\n                            <li class=\"item-wrap\"><i>info@thewizardofink.nl</i></li>\n                            <li class=\"item-wrap\">0640930871</li>\n                        </ul>\n                    </div>\n                    <div class=\"social-wrapper\" mbr-if=\"showSocial\">\n                        <div class=\"social-wrap\">\n                            <div class=\"soc-item\">\n                                <a href=\"https://mobiri.se/\">\n                                    <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\"></span>\n                                </a>\n                            </div>\n                            <div class=\"soc-item\" mbr-if=\"soc > 1\">\n                                <a href=\"https://mobiri.se/\">\n                       